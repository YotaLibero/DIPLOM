{
  "name": "sos",
  "version": "0.1.0",
  "private": true,
  "main": "./public/main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.3.1",
    "fs": "^0.0.1-security",
    "jquery": "^3.6.0",
    "jsdom": "^19.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on localhost:3000 && electron .\"",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/main.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/main.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/main.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "electron": "^19.0.4",
    "cross-env": "^7.0.3",
    "wait-on": "^6.0.1",
    "electron-builder": "^23.0.3"
  },
  "build": {
    "appId": "com.electron.myapp",
    "productName": "My App",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}
